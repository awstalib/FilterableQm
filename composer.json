{
    "name": "awstalib/filterable-qm",
    "version": "1.0.0",
    "description": "Filterable Query or model Package",
    "keywords": ["laravel", "filterable", "query", "pagination"],
    "license": "MIT",
    "authors": [
        {
            "name": "awstalib",
            "email": "ousmoayed@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Awstalib\\FilterableQm\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Awstalib\\FilterableQm\\FilterableQMServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
